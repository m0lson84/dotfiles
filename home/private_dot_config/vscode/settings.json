{
  // General
  "diffEditor.maxComputationTime": 0,
  "explorer.compactFolders": true,
  "explorer.fileNesting.enabled": true,
  "extensions.autoCheckUpdates": true,
  "files.autoSave": "onFocusChange",
  "files.associations": { ".env": "dotenv", ".env.*": "dotenv", "*.rest": "http" },
  "files.exclude": { "**/.git": false, "**/__pycache__": true },
  "notebook.cellToolbarLocation": { "default": "right", "jupyter-notebook": "left" },
  "security.workspace.trust.untrustedFiles": "open",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.commandCenter": true,
  "workbench.colorTheme": "Default Dark Modern",
  "workbench.editorAssociations": { "*.ipynb": "jupyter-notebook" },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.layoutControl.enabled": false,
  "workbench.localHistory.enabled": false,
  "workbench.settings.editor": "json",

  // Editor
  "editor.accessibilitySupport": "off",
  "editor.acceptSuggestionOnEnter": "on",
  "editor.codeActionsOnSave": { "addMissingImports": true, "source.fixAll": true, "source.sortImports": true },
  "editor.detectIndentation": false,
  "editor.fontFamily": "JetBrainsMono Nerd Font",
  "editor.fontSize": 12,
  "editor.fontLigatures": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": false,
  "editor.guides.bracketPairs": "active",
  "editor.inlineSuggest.enabled": true,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.quickSuggestions": { "other": true, "comments": false, "strings": false },
  "editor.snippetSuggestions": "top",
  "editor.suggest.preview": true,
  "editor.suggest.showMethods": true,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.tabSize": 4,
  "editor.tokenColorCustomizations": { "textMateRules": [] },
  "editor.wordWrap": "on",

  // Terminal
  "terminal.external.osxExec": "kitty.app",
  "terminal.external.windowsExec": "C:\\Users\\micha\\AppData\\Local\\Microsoft\\WindowsApps\\wt.exe",
  "terminal.integrated.cwd": "${workspaceRoot}",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.defaultProfile.windows": "pwsh",
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.profiles.linux": {
    "bash": { "path": "/bin/bash", "icon": "terminal-bash" },
    "zsh": { "path": "/usr/bin/zsh", "icon": "terminal" },
    "JavaScript Debug Terminal": null,
    "tmux": null
  },
  "terminal.integrated.profiles.osx": {
    "bash": { "path": "bash", "icon": "terminal-bash" },
    "zsh": { "path": "zsh", "icon": "terminal" },
    "JavaScript Debug Terminal": null,
    "tmux": null
  },
  "terminal.integrated.profiles.windows": {
    "pwsh": { "source": "PowerShell", "icon": "terminal-powershell", "overrideName": true },
    "cmd": { "path": "${env:windir}\\System32\\cmd.exe", "icon": "terminal-cmd", "overrideName": true },
    "Git Bash": null,
    "Command Prompt": null,
    "JavaScript Debug Terminal": null,
    "PowerShell": null,
    "R Terminal": null,
    "Ubuntu (WSL)": null
  },
  "terminal.integrated.shellIntegration.enabled": false,
  "terminal.integrated.tabs.enabled": true,

  // Themes
  "material-icon-theme.activeIconPack": "nest",
  "material-icon-theme.files.associations": {
    "*.decorators.ts": "nest-decorator",
    "*.dto.ts": "nest",
    "*.entity.ts": "database",
    "*.input.ts": "nest-resolver",
    "*.model.ts": "nest-resolver",
    "*.provider.ts": "nest-service",
    "*.repository.ts": "database",
    "*.scalar.ts": "nest-resolver"
  },
  "material-icon-theme.folders.associations": {
    ".azuredevops": "azure-pipelines",
    "analytics": "benchmark",
    "cdk": "aws",
    "devops": "ci",
    "dto": "public",
    "entities": "database",
    "express": "app",
    "http": "public",
    "logger": "log",
    "migrations": "database",
    "nestjs": "app",
    "notebooks": "python",
    "notifications": "messages",
    "repositories": "database",
    "rest": "public",
    "things": "core",
    "workspace": "root"
  },

  // Experimental
  "diffEditor.experimental.showEmptyDecorations": true,
  "diffEditor.experimental.useVersion2": true,

  // Bash
  "shellcheck.exclude": ["1091"],
  "shellformat.effectLanguages": ["shellscript"],
  "shellformat.flag": "-i 2 -ci",
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format",
    "editor.rulers": [100],
    "editor.tabSize": 2
  },

  // C#
  "csharp.format.enable": true,
  "csharp.semanticHighlighting.enabled": true,
  "omnisharp.enableEditorConfigSupport": true,
  "omnisharp.enableImportCompletion": true,
  "omnisharp.enableRoslynAnalyzers": true,
  "omnisharp.organizeImportsOnFormat": true,
  "omnisharp.useEditorFormattingSettings": true,
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp",
    "editor.formatOnType": true,
    "editor.semanticHighlighting.enabled": true
  },

  // DotENV
  "dotenv.enableAutocloaking": false,
  "[dotenv]": {
    "editor.rulers": [120]
  },

  // Go
  "go.lintOnSave": "file",
  "go.lintTool": "golangci-lint",
  "go.lintFlags": ["--fast"],
  "gopls": {
    "formatting.gofumpt": true,
    "ui.diagnostic.analyses": {},
    "ui.diagnostic.staticcheck": true,
    "ui.completion.usePlaceholders": false
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go",
    "editor.rulers": [120]
  },
  "[gotmpl]": {
    "editor.defaultFormatter": "golang.go",
    "editor.rulers": [120]
  },

  // JSON
  "json.schemas": [],
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.rulers": [120]
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.rulers": [120]
  },

  // KDL
  "[kdl]": {
    "editor.rulers": [120]
  },

  // LaTeX
  "latex-workshop.view.pdf.viewer": "tab",
  "[latex]": {
    "editor.defaultFormatter": "James-Yu.latex-workshop",
    "editor.rulers": [120]
  },
  "[lua]": {
    "editor.defaultFormatter": "sumneko.lua"
  },

  // Markdown
  "markdown.styles": ["https://use.fontawesome.com/releases/v5.7.1/css/all.css"],
  "markdown.validate.enabled": true,
  "markdownlint.config": {
    "MD025": { "front_matter_title": "" },
    "MD028": false
  },
  "[markdown]": {
    "editor.codeActionsOnSave": { "source.organizeLinkDefinitions": true },
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
  },

  // Python
  "autoDocstring.docstringFormat": "google-notypes",
  "autoDocstring.startOnNewLine": true,
  "black-formatter.args": ["--line-length", "120"],
  "python.analysis.typeCheckingMode": "basic",
  "python.formatting.provider": "black",
  "python.terminal.activateEnvironment": true,
  "python.testing.pytestEnabled": true,
  "ruff.importStrategy": "fromEnvironment",
  "[python]": {
    "editor.codeActionsOnSave": { "source.fixAll": true, "source.organizeImports": true },
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.rulers": [120]
  },

  // Rust
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer",
    "editor.rulers": [120]
  },

  // Terraform
  "terraform.languageServer.enable": true,
  "[terraform]": {
    "editor.defaultFormatter": "hashicorp.terraform",
    "editor.rulers": [120]
  },

  // TOML
  "evenBetterToml.formatter.columnWidth": 120,
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml",
    "editor.rulers": [120]
  },

  // Typescript /Javascript
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.referencesCodeLens.enabled": true,
  "typescript.referencesCodeLens.showOnAllFunctions": true,
  "typescript.tsserver.log": "off",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.rulers": [120]
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.rulers": [120]
  },

  // YAML
  "redhat.telemetry.enabled": true,
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ],
  "yaml.schemas": {
    "file:///toc.schema.json": "/toc\\.yml/i",
    "file:/users/Michael.Olson/Library/Application Support/Code/User/globalStorage/amazonwebservices.aws-toolkit-vscode/sam.schema.json": [
      "src/constructs/thingpress/thingpress/test/cleanpress/template.yaml",
      "src/constructs/thingpress/thingpress/template.yaml",
      "src/solutions/thing-press/lambda/template.yaml",
      "src/solutions/thing-press/lambda/test/cleanpress/template.yaml"
    ],
    "file:///users/Michael.Olson/Library/Application%20Support/Code/User/globalStorage/amazonwebservices.aws-toolkit-vscode/sam.schema.json": [
      "src/solutions/thing-press/lambda/test/cleanpress/template.yaml",
      "src/solutions/thing-press/lambda/template.yaml"
    ]
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  // Code Spell Checker
  "cSpell.userWords": [
    "devcontainers",
    "dotfiles",
    "esbenp",
    "gofumpt",
    "golangci",
    "gopls",
    "gotmpl",
    "isort",
    "pylint",
    "vsintellicode"
  ],

  // Dev Containers
  "dev.containers.copyGitConfig": false,
  "dev.containers.defaultExtensions": [
    "bierner.github-markdown-preview",
    "davidanson.vscode-markdownlint",
    "dotenv.dotenv-vscode",
    "eamodio.gitlens",
    "esbenp.prettier-vscode",
    "github.copilot",
    "humao.rest-client",
    "streetsidesoftware.code-spell-checker",
    "visualstudioexptteam.vscodeintellicode"
  ],
  "dev.containers.defaultFeatures": {
    "ghcr.io/devcontainers-contrib/features/starship:1": {}
  },
  "dev.containers.gitCredentialHelperConfigLocation": "none",
  "dotfiles.repository": "https://github.com/m0lson84/dotfiles.git",

  // ESLint
  "eslint.codeActionsOnSave.mode": "problems",
  "eslint.format.enable": true,
  "eslint.validate": ["javascript", "typescript"],

  // GitHub Copilot
  "github.copilot.enable": { "*": true, "env": false, "json": true, "markdown": true, "yaml": true },
  "github.copilot.editor.enableAutoCompletions": true,
  "github.copilot.inlineSuggest.enable": true,

  // GitHub Pull Request
  "githubPullRequests.pullBranch": "never",

  // GitLens
  "gitlens.codeLens.scopes": ["document", "containers"],
  "gitlens.graph.layout": "editor",
  "gitlens.graph.minimap.enabled": false,
  "gitlens.showWelcomeOnInstall": false,

  // Jest
  "jest.autoRun": "off",
  "jest.jestCommandLine": "npm test --",

  // Prettier
  "prettier.bracketSpacing": true,
  "prettier.printWidth": 120,
  "prettier.semi": true,
  "prettier.singleQuote": false,
  "prettier.tabWidth": 2,
  "prettier.trailingComma": "es5",
  "prettier.useTabs": false
}
